{
    "name": "astro-dark-theme",
    "displayName": "Customized JetBrains Dark Theme",
    "description": "A VS Code theme converted from JetBrains' Dark color scheme",
    "icon": "media/theme-icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/jensastrup/astro-dark-theme"
    },
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.60.0"
    },
    "publisher": "AstroYenz",
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Dark Converted From JetBrains",
                "uiTheme": "vs-dark",
                "path": "./themes/Dark-Converted-From-JetBrains.json"
            }
        ]
    },
    "files": [
        "themes",
        "README.md",
        "package.json",
        "media"
    ],
    "vscodeignore": [
        ".vscode/**",
        ".gitignore",
        "**/*.vsix"
