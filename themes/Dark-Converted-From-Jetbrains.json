{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dark Converted from JetBrains",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1f22",
    "editor.foreground": "#bcbec4",

    "editorCursor.foreground": "#ced0d6",
    "editor.lineHighlightBackground": "#26282e",
    "editorLineNumber.foreground": "#4b5059",
    "editorLineNumber.activeForeground": "#a1a3ab",

    "editorWhitespace.foreground": "#6f737a",
    "editorIndentGuide.background": "#313438",
    "editorIndentGuide.activeBackground": "#666870",

    "gitDecoration.addedResourceForeground": "#73bd79",
    "gitDecoration.deletedResourceForeground": "#6f737a",
    "gitDecoration.modifiedResourceForeground": "#70aeff",

    "terminal.background": "#1e1f22",
    "terminal.foreground": "#bcbec4",
    "terminalCursor.foreground": "#ced0d6",

    "panel.background": "#1e1f22",
    "panel.border": "#2b2d30",
    "sideBar.background": "#1e1f22",
    "sideBar.border": "#2b2d30",
    "titleBar.activeBackground": "#1e1f22",
    "titleBar.activeForeground": "#bcbec4",
    "statusBar.background": "#1e1f22",
    "statusBar.foreground": "#bcbec4",

    "editor.selectionBackground": "#2d543f",
    "editor.selectionHighlightBackground": "#2d543f",
    "search.resultsInfoForeground": "#bcbec4",
    "editor.findMatchBackground": "#114957",
    "editor.findMatchHighlightBackground": "#114957",
    "editor.findRangeHighlightBackground": "#114957"
  },
  "tokenColors": [
    //
    // --- Invalid / Bad Characters ---
    //
    {
      "scope": ["invalid.illegal"],
      "settings": {
        "foreground": "#f75464",
        "fontStyle": "underline"
      }
    },

    //
    // --- Comments ---
    //
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#7a7e85"
      }
    },
    {
      "scope": [
        "comment.block.documentation",
        "comment.block.javadoc"
      ],
      "settings": {
        "foreground": "#5f826b"
      }
    },

    //
    // --- Strings & Escapes ---
    //
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#6aab73"
      }
    },
    {
      "scope": ["string.escape"],
      "settings": {
        "foreground": "#cf8e6d"
      }
    },
    {
      "scope": ["invalid.escape"],
      "settings": {
        "foreground": "#f75464"
      }
    },

    //
    // --- Keywords ---
    //
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#cf8e6d"
      }
    },

    //
    // --- Numbers ---
    //
    {
      "scope": [
        "constant.numeric",
        "keyword.operator.units"
      ],
      "settings": {
        "foreground": "#2aacb8"
      }
    },

    //
    // --- Constants ---
    //
    {
      "scope": [
        "constant.language",
        "constant.character",
        "constant.other",
        "variable.other.enummember",
        "support.constant"
      ],
      "settings": {
        "foreground": "#c77dbb",
        "fontStyle": "bold"
      }
    },

    //
    // --- Functions / Methods ---
    //
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#56a8f5"
      }
    },

    //
    // --- Classes, Types ---
    //
    {
      "scope": [
        "entity.name.type",
        "support.class",
        "support.type",
        "storage.type.cs",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#2fbaa3"
      }
    },

    //
    // --- Variables ---
    //
    {
      "scope": [
        "variable",
        "entity.name.variable",
        "meta.definition.variable.name",
        "support.variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#bcbec4"
      }
    },

    //
    // --- HTML / XML Tags ---
    //
    {
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag",
        "meta.tag.html",
        "meta.tag.xml"
      ],
      "settings": {
        "foreground": "#d5b778"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attributes"
      ],
      "settings": {
        "foreground": "#bcbec4"
      }
    },

    //
    // --- JSX/TSX Components ---
    //
    {
      "scope": [
        "support.class.component.tsx",
        "support.class.component.jsx"
      ],
      "settings": {
        "foreground": "#9c9cff"
      }
    },

    //
    // --- RegExp ---
    //
    {
      "scope": [
        "string.regexp",
        "constant.character.escape.backslash.regexp"
      ],
      "settings": {
        "foreground": "#42c3d4"
      }
    }
  ]
}